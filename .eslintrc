{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "babel"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "react/prop-types": 0,
    "no-unused-vars": ["error", { "ignoreRestSiblings": true }]
  }
}
